 <div class="container dwcm-collectionobject-view">
    <div class="page-header">
        <h1>
            {{t 'user.list.header'}}
        </h1>
    </div>
    <div class='panel panel-default'>
        <div class="panel-body">
            <div class="row">
                <div class="col-xs-12 col-md-12 col-lg-12">
                    <table class="table table-striped table-hover">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>{{t 'fields.labels.user.first_name'}}</th>
                                <th>{{t 'fields.labels.user.last_name'}}</th>
                                <th>{{t 'fields.labels.user.email'}}</th>
                                <th>{{t 'fields.labels.user.username'}}</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each model as |user|}} 
                            	 {{#link-to 'users.view' user tagName='tr' class='clickable'}}
                                    <td>
                                        {{user.id}}
                                    </td>
                                    <td>
                                        {{user.first_name}} 
                                    </td>
                                    <td>
                                        {{user.last_name}}
                                    </td> 
                                    <td>
                                        {{user.email}}
                                    </td> 
                                    <td>
                                        {{user.username}}
                                    </td>  
                                    <br>
                                {{/link-to}}
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>